{
    "version":2,
    "name":"ChatSphere",
    "builds":[
        {
            "src":"./index.js",
            "use":"@vercel/node"
        }
    ],
    "routes":[
        {
            "src": "/socket.io/(.*)",  
            "dest": "./index.js"
        },
        {
            "src":"/api/(.*)",
            "dest":"./index.js"
        }
    ]
}